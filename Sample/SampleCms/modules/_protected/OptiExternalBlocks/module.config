<?xml version="1.0" encoding="utf-8" ?>
<module loadFromBin="true" clientResourceRelativePath="ClientResources" viewEngine="Razor"
        authorizationPolicy="OptiExternalBlocks:Security:Policy"
        moduleJsonSerializerType="None" prefferedUiJsonSerializerType="Net">
  <assemblies>
    <add assembly="OptiExternalBlocks" />
  </assemblies>
  <clientResources>
    <add name="opti-external-blocks-styles" path="Styles/externalcontent.css" resourceType="Style" />
  </clientResources>
  <dojo>
    <paths>
      <add name="opti-external-blocks" path="opti-external-blocks" />
    </paths>
  </dojo>
  <clientModule>
    <moduleDependencies>
      <add dependency="CMS" type="RunAfter" />
      <add dependency="Shell" type="RunAfter" />
    </moduleDependencies>
  </clientModule>
</module>
